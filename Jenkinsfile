pipleline{
    agent none
    stages {
        stage('Developing') {
            steps {
                script {
                    echo "Developing build, going to test next"
                }
            }
        }

        stage('Testing') {
            steps {
                script {
                    echo 'Doing testing soon, results are going to come!!!'
                }
            }
        }

        stage('Final_Recheck') {
            steps {
                script {
                    echo 'Doing Rechecks soon launching in Production env!!!'
                }
            }
        }
    }
}
